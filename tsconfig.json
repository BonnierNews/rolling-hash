{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "inlineSourceMap": false,
    "listEmittedFiles": false,
    "listFiles": false,
    "moduleResolution": "Node",
    "downlevelIteration": true,
    "pretty": true,
    "rootDir": ".",
    "strict": true,
    "traceResolution": false,
    "types": ["Node"]
  },
  "compileOnSave": false,
  "exclude": ["node_modules", "dist", "test"],
  "include": ["index.ts"]
}
